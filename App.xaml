<Application
    x:Class="About_You.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:About_You">

    <Application.Resources>
        <ResourceDictionary>
            <Style x:Key="TitleTextBlockStyle" TargetType="TextBlock">
                <!-- Definisi gaya untuk judul TextBlock -->
                <Setter Property="FontSize" Value="16"/>
                <Setter Property="Margin" Value="0,0,0,5"/>
                <!-- Tambahkan properti gaya lainnya sesuai kebutuhan -->
            </Style>

            <Style x:Key="TextInputStyle" TargetType="TextBox">
                <!-- Definisi gaya untuk TextBox -->
                <Setter Property="Margin" Value="0,0,0,10"/>
                <!-- Tambahkan properti gaya lainnya sesuai kebutuhan -->
            </Style>

            <Style x:Key="ButtonStyle" TargetType="Button">
                <!-- Definisi gaya untuk tombol -->
                <!-- Tambahkan properti gaya sesuai kebutuhan -->
            </Style>

            <Style x:Key="OutputTextBlockStyle" TargetType="TextBlock">
                <!-- Definisi gaya untuk TextBlock output -->
                <!-- Tambahkan properti gaya sesuai kebutuhan -->
            </Style>
        </ResourceDictionary>
    </Application.Resources>

</Application>
